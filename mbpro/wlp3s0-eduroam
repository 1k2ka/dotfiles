Connection='wireless'
Interface=wlp3s0
Security='wpa-configsection'
Description="eduroam network"
IP='dhcp'
TimeoutWPA=30
WPAConfigSection=(
    'ssid="eduroam"'
    'proto=WPA2'
    'key_mgmt=WPA-EAP'
    'eap=PEAP'
    'phase2="auth=MSCHAPV2"'
    'ca_cert="/etc/ssl/certs/AddTrust_External_CA_Root.pem"'
    'domain_suffix_match="liu.se"'
    'anonymous_identity="anonymous@liu.se"'    
    'identity="henan076@liu.se"'
    'password=""'
)
